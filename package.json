{
  "name": "msfs2020-map-enhancement",
  "author": {
    "name": "He Sicong"
  },
  "description": "Microsoft Flight Simulator 2020 map enhancement tool",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore src/",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json",
    "typecheck": "tsc --noEmit",
    "build:python": "pwsh -ExecutionPolicy Bypass -File extra/server/scripts/package.ps1",
    "build:nginx": "pwsh -ExecutionPolicy Bypass -File extra/nginx/scripts/package.ps1",
    "build:service-wrappers": "pwsh -ExecutionPolicy Bypass -File extra/compile-service-wrappers.ps1",
    "electron:build": "npm run build:nginx && npm run build:python && npm run build:service-wrappers && vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.11.4",
    "@vicons/ionicons5": "^0.12.0",
    "core-js": "^3.35.0",
    "dns": "^0.2.2",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^4.6.5",
    "got": "^11.8.6",
    "hpagent": "^0.1.2",
    "http2": "^3.3.7",
    "leaflet": "^1.9.4",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "sudo-prompt": "^9.2.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "winlink": "^1.0.2"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "electron": "13.6.6",
    "electron-builder": "^22.14.12",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "naive-ui": "^2.21.3",
    "prettier": "^2.8.8",
    "sass": "^1.70.0",
    "sass-loader": "^8.0.2",
    "typescript": "~4.9.5",
    "vfonts": "^0.1.0",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  }
}
