{
  "name": "msfs2020-map-enhancement",
  "author": {
    "name": "He Sicong"
  },
  "description": "Microsoft Flight Simulator 2020 map enhancement tool",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:python": "pwsh -ExecutionPolicy Bypass -File extra/server/scripts/package.ps1",
    "build:nginx": "pwsh -ExecutionPolicy Bypass -File extra/nginx/scripts/package.ps1",
    "build:service-wrappers": "pwsh -ExecutionPolicy Bypass -File extra/compile-service-wrappers.ps1",
    "electron:build": "npm run build:nginx && npm run build:python && npm run build:service-wrappers && vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "^2.11.4",
    "@vicons/ionicons5": "^0.11.0",
    "core-js": "^3.6.5",
    "dns": "^0.2.2",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.1",
    "got": "^11.8.3",
    "hpagent": "^0.1.2",
    "http2": "^3.3.7",
    "leaflet": "^1.7.1",
    "minimist": "^1.2.6",
    "moment": "^2.29.2",
    "sudo-prompt": "^9.2.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "winlink": "^1.0.2"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "electron": "13.6.9",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "naive-ui": "^2.37.3",
    "prettier": "^2.8.8",
    "sass": "^1.69.7",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vfonts": "^0.1.0",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  }
}
